<% conversation.participants.each do |participant| %>
  <% unless participant == current_user %>
    <% if participant.pic_file_name %>
                <%= image_tag participant.pic.url(:thumb),:class=>"tile-image big-illustration" %>
            <% else %>
                 <%= image_tag  "logo.png",:class=>"tile-image big-illustration" %>
            <% end %>
            
  <% end %>
<% end %>